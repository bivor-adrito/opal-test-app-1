{
  "name": "ci-cmp-bynder-middleware",
  "version": "1.0.0",
  "description": "Bynder x Optimizely CMP middleware",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@helper": "dist/helper",
    "@modules": "dist/modules",
    "@routes": "dist/routes",
    "@middlewares": "dist/middlewares"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@bynder/bynder-js-sdk": "^2.3.9",
    "@optimizely-opal/opal-tools-sdk": "^0.1.2-dev",
    "@sentry/node": "^9.6.0",
    "@welcomesoftware/cmp-sdk": "^1.0.5",
    "body-parser": "^1.20.2",
    "bullmq": "^5.41.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.5.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "pino": "^9.5.0",
    "pino-pretty": "^12.1.0",
    "winston": "^3.14.2",
    "xml2js": "^0.6.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/joi": "^17.2.2",
    "@types/morgan": "^1.9.9",
    "@types/xml2js": "^0.4.14",
    "axios": "^1.7.7",
    "faker": "^5.1.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^2.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
